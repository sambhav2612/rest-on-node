{
  "name": "rest-on-node",
  "version": "0.0.2",
  "description": "restful api in node",
  "main": "server.js",
  "scripts": {
    "test": "npm --version",
    "run": "node server.js",
    "server": "nodemon ./server.js localhost 8888",
    "start": "node server.js",
    "client": "cd frontend && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
