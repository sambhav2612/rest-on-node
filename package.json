    "express": "^4.16.2",
    "mongoose": "^6.0.4",
    "nodemon": "^1.15.1"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sambhav2612/rest-on-node.git"
  },
